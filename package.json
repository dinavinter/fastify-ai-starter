{
  "name": "fastify-ai-starter",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@9.6.0",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "build": "npx pnpm build:pkgroll",
    "build:tsup": "pnpx tsup ./src/app.ts --outDir ./dist --target=esnext --format esm --treeshake --shims --publicDir  ",
    "build:pkgroll": "pnpx pkgroll --clear-dist --dist ./dist  --format=esm --target=esnext --treeshake --shims    && cp -r ./public/ ./dist ",
    "dev": "pnpx tsx --env-file=.env ./src/index.ts",
    "vercel:dev": "pnpm build && vercel dev --listen 5005",
    "vercel:deploy": "pnpm build &&  vercel deploy",
    "cf:deploy": "pnpm build && cf push",
    "start": "node ./dist/index.mjs"

  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.54",
    "ai": "^3.3.19",
    "cf-nodejs-logging-support": "^7.3.0",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "fastify-sse-v2": "^4.0.0"
  },
  "devDependencies": {
    "@fastify/one-line-logger": "^1.4.0",
    "@fastify/swagger": "^8.15.0",
    "@scalar/fastify-api-reference": "^1.25.7",
    "@tsconfig/node22": "^22.0.0",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^22.5.0",
    "@vercel/functions": "^1.4.1",
    "@vercel/node": "^3.2.10",
    "fastify-type-provider-zod": "^2.0.0",
    "js-beautify": "^1.15.1",
    "tsx": "^4.18.0",
    "typescript": "^5.5.4",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5"
  },
  "main": "./dist/app.cjs",
  "module": "./dist/app.mjs",
  "types": "./dist/app.d.cts",
  "exports": {
    "app": {
      "require": {
        "types": "./dist/app.d.cts",
        "default": "./dist/app.cjs"
      },
      "import": {
        "types": "./dist/app.d.mts",
        "default": "./dist/app.mjs"
      }
    },
    "serverless": {
      "require": "./dist/serverless.cjs",
      "import": "./dist/serverless.mjs",
      "types": "./dist/serverless.d.ts"
    },
    "dev": {
      "require": "./dist/dev.cjs",
      "import": "./dist/dev.mjs",
      "types": "./dist/dev.d.ts"
    },
    "index":{
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
    }
  },
  "bin": "./dist/dev.cjs"
}